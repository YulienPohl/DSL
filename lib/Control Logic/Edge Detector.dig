        </entry>
      </elementAttributes>
      <pos x="0" y="-140"/>
    </visualElement>
    <visualElement>
      <elementName>GenericCode</elementName>
      <elementAttributes>
        <entry>
          <string>generic</string>
          <string>if (args.edge = 1) {
    addWire(12, -2, 13, -2);
} else {
    addWire(12, 0, 13, 0);
}</string>
        </entry>
      </elementAttributes>
      <pos x="120" y="-220"/>
    </visualElement>
    <visualElement>
      <elementName>Testcase</elementName>
      <elementAttributes>
        <entry>
          <string>Label</string>
          <string>rising</string>
        </entry>
        <entry>
          <string>Testdata</string>
          <testData>
            <dataString>C D  e
0 0  0
C 0  0

C 1  1
C 0  0

C 1  1
C 1  0
C 0  0
C 1  1
C 0  0</dataString>
          </testData>
        </entry>
        <entry>
          <string>generic</string>
          <string>edge := 1;</string>
        </entry>
      </elementAttributes>
      <pos x="0" y="-280"/>
    </visualElement>
