    </visualElement>
    <visualElement>
      <elementName>And</elementName>
      <elementAttributes>
        <entry>
          <string>generic</string>
          <string>if (args.edge = 1) {
    this.inverterConfig = [&quot;In_2&quot;];
} else {
    this.inverterConfig = [&quot;In_1&quot;];
}</string>
        </entry>
      </elementAttributes>
